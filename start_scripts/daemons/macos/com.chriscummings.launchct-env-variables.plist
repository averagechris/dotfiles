<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.chriscummings.launchctl-env-variables</string>
    <key>ProgramArguments</key>
    <array>
        <string>/bin/sh</string>
        <string>-c</string>
        <string>launchctl setenv WORKON_HOME $HOME/.local/share/virtualenvs/ PATH $PATH:/usr/local/bin:$HOME/dotfiles/bin:$HOME/.cargo/bin:$HOME/.pyenv/:$HOME/.pyenv/versions:$HOME/.pyenv/shims</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
</dict>
</plist>
